{
  "name": "gather-mod",
  "version": "0.0.1",
  "description": "A collection of client-side mods for Gather.town",
  "main": "dist/index.js",
  "scripts": {
    "build": "esbuild ./src --outfile=./dist/index.js --bundle --sourcemap --target=chrome58 --packages=external",
    "build:prod": "esbuild ./src --outfile=./dist/index.js --bundle --minify --target=chrome58 --packages=external",
    "format": "prettier --write \"src/**/*.+(js|jsx|ts|tsx|md|html|json)\"",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": ["gather", "mod", "lol"],
  "author": "Punnawut Khowkittipaiboon <me@itskaynine.com>",
  "license": "MIT",
  "repository": "github:itsKaynine/gather-mod",
  "homepage": "https://github.com/itsKaynine/gather-mod",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "esbuild": "^0.17.18",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  }
}
